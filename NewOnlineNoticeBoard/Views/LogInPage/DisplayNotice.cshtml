@model IEnumerable<NewOnlineNoticeBoard.Models.CE_Notice>

@{
    ViewBag.Title = "DisplayNotice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DisplayNotice</h2>
@ViewBag.msg
@ViewBag.data


<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TitleOfNotice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoticeType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FileName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TitleOfNotice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NoticeType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FileName)
                </td>
                @*<td>
                    @Html.ActionLink("Delete", "Delete", new { id = item.NoticeId })
                </td>*@
            </tr>
        }
</table>
